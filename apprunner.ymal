# apprunner.yaml (Final Configuration)
version: 1.0

runtime: nodejs18 

build:
  commands:
    # Build Command: dependencies को backend फ़ोल्डर में इंस्टॉल करें
    - npm install --prefix backend

run:
  command: node backend/server.js
  
  port: 3002

healthcheck:
  path: /api/status
  protocol: HTTP