# API Keys for SEO Services
PAGE_SPEED_INSIGHTS_API_KEY=your_pagespeed_api_key_here
WHOAPI_KEY=your_whoapi_key_here
SAFE_BROWSING_API_KEY=your_safebrowsing_api_key_here

# Server Configuration
PORT=3000
NODE_ENV=development

# Database Configuration
DB_PATH=./data/scans.db

# Queue Configuration
REDIS_URL=redis://localhost:6379
